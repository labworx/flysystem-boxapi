{
    "name": "labworx/flysystem-boxapi",
    "description": "Flysystem Adapter for the Box.com API",
    "keywords": [
        "entidia",
        "flysystem-boxapi",
        "flysystem",
        "box.com",
        "api"
    ],
    "homepage": "https://github.com/labworx/flysystem-boxapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Deuerlein",
            "email": "deuerlein@entidia.de",
            "homepage": "https://entidia.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "league/flysystem": "^1.0.20",
        "maengkom/box": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Labworx\\FlysystemBoxApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Labworx\\FlysystemBoxApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
